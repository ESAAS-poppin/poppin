<% provide :head_tags do %>
    <meta name='turbolinks-visit-control' content='reload'>
    <script>
    document.addEventListener("DOMContentLoaded", function(){
        initMultiMapPage(<%= raw @venues %>)

    });
    </script>
<% end %>
<h2 class="page-title login-title">Venues Map View</h2>
<% if session[:user_type] == 'user' -%>
    <%= link_to "My Dashboard", user_path(session[:user_id]) %>
<% else -%>
    <%= link_to "My Dashboard", venue_admin_path(session[:user_id]) %>
<% end -%>
<div class="Events-mapWrapper">
    <div class="Venues-map">
        <div id="map" /div>
    </div>
</div>